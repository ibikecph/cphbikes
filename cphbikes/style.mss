@water: hsl( 195, 95%, 75% );
@land: hsl( 40, 30%, 92% );
@park: hsl( 75, 95%, 60% );
@farmland: hsl( 60, 60%, 90% );
@sand: hsl( 50, 80%, 90% );

@road: white;

@runway: hsl( 180, 8%, 92% );
@playground: hsl( 50, 100%, 50% );
@graveyard: darken( @park, 10% );
@residential: darken( @land, 3% );


@font: 'Arial Regular';
@font_bold: 'Arial Bold';

@scale: 1;

Map {
  background-color: @water;
}