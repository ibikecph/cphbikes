[zoom=10] {
  #z10-roads[kind='highway'][render='inline'] {
    line-width: 2;
  }

  #z10-roads[kind='highway'][render='outline'] {
    line-width: 3.5;
  }

  #z10-roads[kind='major_road'][render='outline'] {
    line-width: 2;
  }

  #z10-roads[kind='major_road'][render='inline'] {
    line-width: 1;
  }
}
